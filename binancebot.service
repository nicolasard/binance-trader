#SystemD file to install the java app as a service

[Unit]
Description=Binance bot application
After=network-online.target

[Service]
ExecStart=/opt/binance-bot/jdk-11.0.11+9-jre/bin/java -jar /opt/binance-bot/binancebot-0.0.1-SNAPSHOT.jar

[Install]
WantedBy=multi-user.target
Alias=binancebot.service
